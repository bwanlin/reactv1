{
  "name": "react-app",
  "version": "1.0.0",
  "description": "React webpack",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -d --env.platform=default --progress --hot",
    "build": "NODE_ENV='production' webpack -p --progress --hot",
    "test": "jest --config jest.config.json --updateSnapshot"
  },
  "keywords": [],
  "author": "",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-import": "^1.6.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "prop-types": "^15.5.10",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "antd": "^3.0.1",
    "jquery": "^3.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
